
#DON'T SHOW DIRECTORY LISTINGS
Options -Indexes

#FOLLOW SYMBOLIC LINKS
Options +FollowSymLinks

#SET DEFAULT HANDLER
DirectoryIndex index.php index.html index.htm

RewriteEngine on

#WWW REWRITE
RewriteCond %{HTTP_HOST} ^airytek\.com$ [NC]
RewriteRule ^(.*)$ http://www.airytek.com/$1 [L,R=301]


#ERROR 404 PAGE
ErrorDocument 404 /404.html
